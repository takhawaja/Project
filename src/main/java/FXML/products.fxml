<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="456.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.uh.tech.cis3368.semesterproject.ProductController">
   <children>
      <ListView fx:id="productList" layoutX="351.0" layoutY="58.0" onMouseClicked="#handleMouseClicked" prefHeight="307.0" prefWidth="220.0" />
      <TextField fx:id="productname" layoutX="151.0" layoutY="75.0" />
      <Label layoutX="39.0" layoutY="77.0" text="Product Name">
         <font>
            <Font name="MV Boli" size="12.0" />
         </font></Label>
      <Label layoutX="214.0" layoutY="14.0" prefHeight="46.0" prefWidth="172.0" text="Product Page">
         <font>
            <Font name="MV Boli" size="26.0" />
         </font>
      </Label>
      <Button fx:id="btnAddProduct" layoutX="138.0" layoutY="282.0" mnemonicParsing="false" onAction="#UpdateButton" prefHeight="30.0" prefWidth="149.0" text="Add Product">
         <font>
            <Font name="MV Boli" size="12.0" />
         </font></Button>
      <Button fx:id="btnDeleteProduct" layoutX="137.0" layoutY="339.0" mnemonicParsing="false" onAction="#deleteProduct" prefHeight="25.0" prefWidth="149.0" text="Remove">
         <font>
            <Font name="MV Boli" size="12.0" />
         </font></Button>
      <Button fx:id="btnDon" layoutX="406.0" layoutY="377.0" mnemonicParsing="false" onAction="#handleClear" prefHeight="25.0" prefWidth="109.0" text="Clear">
         <font>
            <Font name="Palatino Linotype" size="12.0" />
         </font></Button>
      <Button fx:id="btnDone" layoutX="189.0" layoutY="390.0" mnemonicParsing="false" onAction="#handleDone1" text="Done">
         <font>
            <Font name="MV Boli" size="12.0" />
         </font></Button>
      <Label layoutX="39.0" layoutY="117.0" text="Product Quantity">
         <font>
            <Font name="MV Boli" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="productquantity" layoutX="151.0" layoutY="115.0" />
   </children>
</AnchorPane>
